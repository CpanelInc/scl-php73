{
    "vars" : {
       "pkg_php_version": "%{version}",
        "ea_apr_name": "ea_apr16",
        "ea_apr_dir": "/opt/cpanel/ea-apr16",
        "ea_apu_dir": "/opt/cpanel/ea-apr16",
        "_httpd_mmn": "20120211x8664",
        "_httpd_dir": "/etc/apache2",
        "_httpd_bindir": "%{_httpd_dir}/bin",
        "_httpd_modconfdir": "%{_httpd_dir}/conf.modules.d",
        "_httpd_confdir": "%{_httpd_dir}/conf.d",
        "_httpd_contentdir": "/usr/share/apache2",
        "_httpd_moddir": "/usr/lib64/apache2/modules",
        "__perl": "/usr/bin/perl",
        "vmmn": "",
        "ea_apr_config": "/opt/cpanel/ea-apr16/bin/apr-1-config"
    },
    "override_vars": {
        "_httpd_apxs": "/usr/bin/apxs"
    },
    "remove_from_provides": [
        "Provides:"
    ],
    "remove_from_requires": [
        "pkgconfig(ea-libzip)",
        "pkgconfig(libzip)",
        "scl-utils-build",
        "autotools-latest-autoconf",
        "ea-php73-libc-client-devel",
        "ea-php73-libc-client"
    ],
    "add_to_build_requires": [
        "pkg-config",
        "libc-client2007e-dev",
        "ea-apache24-mod-mpm-prefork",
        "libonig-dev",
        "libsystemd0",
        "libpspell-dev"
    ],
    "remove_from_install": [
        "pdo_odbc.so"
    ],
    "remove_from_patches": [
        "0010-Add-AUTOCONF-AUTOHEADER-variables-for-C6.patch"
    ],
    "fix_dh_usrlocal": 1,
    "add_to_requires": {
        "ea-php73-php-cli": [ "libfbclient2" ],
        "ea-php73-php-dbg": [ "libfbclient2" ],
        "ea-php73-php-fpm": [ "libfbclient2" ],
        "ea-php73-php-litespeed": [ "libfbclient2" ],
        "ea-php73-php-pgsql": [ "libpq5" ],
        "ea-php73-php-dba": [ "libtokyocabinet9" ],
        "ea-php73-php-pspell": [ "libaspell15" ],
        "ea-php73-php-enchant": [ "libenchant1c2a" ],
        "ea-php73-php-imap": [ "libc-client2007e" ]
    }
}
